{
  "name": "devops-strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "seed:example": "node ./scripts/seed.js",
    "start": "strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:admin": "playwright test --grep=\"@admin\"",
    "test:api": "playwright test --grep=\"@api\"",
    "test:auth": "playwright test --grep=\"@auth\"",
    "test:content": "playwright test --grep=\"@content\"",
    "test:smoke": "playwright test --grep=\"@smoke\"",
    "test:regression": "playwright test --grep=\"@regression\"",
    "test:ci": "playwright test --reporter=github",
    "test:docker": "docker run --rm -v $(pwd):/workspace -w /workspace mcr.microsoft.com/playwright:focal npm run test"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.12.3",
    "@strapi/plugin-users-permissions": "5.12.3",
    "@strapi/strapi": "5.12.3",
    "better-sqlite3": "11.3.0",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.27",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.53.0",
    "@types/node": "^20.19.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "319879ee-b666-442c-bb8e-1f29a9b75138"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "better-sqlite3",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core",
      "core-js-pure"
    ]
  }
}
